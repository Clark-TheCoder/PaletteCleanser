{
  "manifest_version": 3,
  "name": "Palette Cleanser",
  "version": "1.0",
  "description": "Colour Season Cleanser",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "24": "/media/iconLg.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script/pageScanner.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": []
}
